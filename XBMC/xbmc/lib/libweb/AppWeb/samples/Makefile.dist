DIRS	= C C++

all:	compile

compile:
	@set -e ; \
	for f in $(DIRS) ; \
	do \
		echo "    make -C $$f compile" ; \
		make --no-print-directory -C "$$f" compile ;\
	done

clean:
	@for f in $(DIRS) ; \
	do \
		echo "    make -C $$f clean" ; \
		make --no-print-directory -C "$$f" clean ; \
	done

