8bit_tab_to_h : 8bit_tab_to_h.c
	gcc -O -Wall -g $< -o $@

cjk_tab_to_h : cjk_tab_to_h.c
	gcc -O -Wall -g $< -o $@

cjk_variants : cjk_variants.c
	gcc -O -Wall -g $< -o $@


clean : force
	rm -f 8bit_tab_to_h cjk_tab_to_h cjk_variants $(ALL) GBKext.TXT

force :

