+#endif
+
+  putenv(\"TZ=EST+5EDT,M4.1.0,M10.5.0\");
+  tzset();
+  if (localtime(&groundhogday)->tm_hour != 19)
+      exit(1);
+#if HAVE_TZNAME
+  if (strcmp(tzname[0], \"EST\") || strcmp(tzname[1], \"EDT\"))
