+    set_target_properties(${name} PROPERTIES
+        OUTPUT_NAME ${LIBPYTHON}${ABIFLAGS}
+        LIBRARY_OUTPUT_DIRECTORY ${PROJECT_BINARY_DIR}/${LIBPYTHON_LIBDIR}
+        RUNTIME_OUTPUT_DIRECTORY ${PROJECT_BINARY_DIR}/${LIBPYTHON_LIBDIR}
