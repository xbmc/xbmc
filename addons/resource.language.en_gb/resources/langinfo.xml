      <decimalseparator>.</decimalseparator>
    </region>

    <region name="Canada" locale="CA">
      <dateshort>YYYY-MM-DD</dateshort>
      <datelong>DDDD, MMMM D, YYYY</datelong>
      <time symbolAM="AM" symbolPM="PM">h:mm:ss xx</time>
