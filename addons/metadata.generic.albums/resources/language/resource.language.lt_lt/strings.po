msgctxt "#30000"
msgid "Preferences"
msgstr "Nustatymai"

msgctxt "#30001"
msgid "Preferred language for album review"
msgstr "Pageidaujama albumų apžvalgos kalba"

msgctxt "#30002"
msgid "Prefer genres from"
msgstr "Pirmenybė žanrams iš"

msgctxt "#30003"
msgid "Prefer styles from"
msgstr "Pirmenybė stiliams iš"

msgctxt "#30004"
