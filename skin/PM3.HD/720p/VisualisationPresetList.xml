			<texturefocus>close-windowFO.png</texturefocus>
			<texturenofocus>close-window.png</texturenofocus>
			<onleft>2</onleft>
			<onright>2</onright>
			<onup>2</onup>
			<ondown>2</ondown>
			<visible>system.getbool(lookandfeel.enablemouse)</visible>
