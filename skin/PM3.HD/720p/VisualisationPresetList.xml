			<height>610</height>
			<texture border="40">DialogFront.png</texture>
		</control>
		<control type="label" id="3">
			<description>heading label</description>
			<posx>40</posx>
			<posy>40</posy>
			<width>720</width>
			<height>30</height>
			<align>center</align>
			<aligny>center</aligny>
