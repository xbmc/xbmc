		</control>
		<control type="spincontrol" id="61">
			<description>Next page button</description>
			<posx>690</posx>
			<posy>538</posy>
			<subtype>page</subtype>
			<font>font12b</font>
			<onleft>2</onleft>
			<onright>2</onright>
			<ondown>2</ondown>
			<onup>2</onup>
			<textcolor>-</textcolor>
			<showonepage>false</showonepage>
		</control>
	</controls>
