<window>
  <id>2006</id>
  <defaultcontrol>2</defaultcontrol>
  <allowoverlay>no</allowoverlay>
  <controls>
    <control>
      <description>visualisation</description>
      <type>visualisation</type>
      <id>2</id>
      <posx>0</posx>
      <posy>0</posy>
      <width>720</width>
      <height>576</height>
    </control>

    <!-- codec & viz infos -->
    <control>
      <description>preset background image</description>
      <type>image</type>
      <id>0</id>
      <posx>-10</posx>
      <posy>40</posy>
      <width>740</width>
      <height>65</height>
      <texture>osd_menu_bg.png</texture>
      <visible effect="fade" time="300">Player.ShowCodec</visible>
    </control>

    <control>
      <description>Codecinfo</description>
      <type>label</type>
      <id>0</id>
      <width>600</width>
      <posx>75</posx>
      <posy>50</posy>
      <label>Audio | Codec: $INFO[musicplayer.Codec] - Bitrate: $INFO[musicplayer.Bitrate]kbps - Bits: $INFO[musicplayer.bitspersample]bit - Freq.: $INFO[musicplayer.Samplerate]kHz - Channels: $INFO[musicplayer.Channels]</label>
      <align>left</align>
      <font>font12</font>
      <visible effect="fade" time="300">Player.ShowCodec</visible>
    </control>

    <control>
      <description>Presets label</description>
      <type>label</type>
      <id>0</id>
      <posx>75</posx>
      <posy>75</posy>
      <width>560</width>
      <label>Visuals | fps: $INFO[System.FPS] - </label>
      <font>font12</font>
      <align>left</align>
      <visible effect="fade" time="300">Player.ShowCodec</visible>
    </control>

    <control>
      <description>Presets label</description>
      <type>label</type>
      <id>0</id>
      <posx>225</posx>
      <posy>75</posy>
      <width>420</width>
      <label>$INFO[Visualisation.Name] - $INFO[Visualisation.Preset]</label>
      <font>font12</font>
      <align>left</align>
      <visible effect="fade" time="300">Player.ShowCodec</visible>
    </control>

    <control>
      <type>image</type>
      <description>Preset locked image</description>
      <id>0</id>
      <posx>210</posx>
      <posy>76</posy>
      <width>12</width>
      <height>15</height>
      <visible effect="fade" time="300">Visualisation.Locked + Player.ShowCodec</visible>
      <texture>arenaitem-private.png</texture>
    </control>

    <!-- media infos -->
    <control>
      <description>media info background image</description>
      <type>image</type>
      <id>1</id>
      <posx>-10</posx>
      <posy>455</posy>
      <width>740</width>
      <height>100</height>
      <visible effect="fade" time="1000">Player.ShowInfo</visible>
      <texture>osd_menu_bg.png</texture>
    </control>

    <control>
      <description>cover image</description>
      <type>image</type>
      <id>1</id>
      <posx>75</posx>
      <posy>455</posy>
      <width>100</width>
      <height>100</height>
      <info>MusicPlayer.Cover</info>
      <visible effect="fade" time="1000">Player.ShowInfo</visible>
      <keepaspectratio>true</keepaspectratio>
    </control>

    <control>
      <description>Artist label</description>
      <type>label</type>
      <id>1</id>
      <width>445</width>
      <posx>180</posx>
      <posy>458</posy>
      <info>MusicPlayer.Artist</info>
      <visible effect="fade" time="1000">Player.ShowInfo</visible>
      <align>left</align>
      <font>special12</font>
    </control>

    <control>
      <description>Title label</description>
      <type>label</type>
      <id>1</id>
      <width>430</width>
      <posx>180</posx>
      <posy>479</posy>
      <info>MusicPlayer.Title</info>
      <visible effect="fade" time="1000">Player.ShowInfo</visible>
      <align>left</align>
      <font>special13</font>
    </control>

    <control>
      <description>Album label</description>
      <type>label</type>
      <id>1</id>
      <width>430</width>
      <posx>180</posx>
      <posy>507</posy>
      <info>MusicPlayer.Album</info>
      <visible effect="fade" time="1000">Player.ShowInfo</visible>
      <align>left</align>
      <font>font10</font>
    </control>

    <control>
      <description>time label</description>
      <type>label</type>
      <id>1</id>
      <width>430</width>
      <posx>180</posx>
      <posy>528</posy>
      <label>$INFO[musicplayer.Time] / $INFO[musicplayer.Duration]</label>
      <visible effect="fade" time="1000">Player.ShowInfo</visible>
      <align>left</align>
      <font>font10</font>
    </control>

    <control>
      <description>Playlist</description>
      <type>label</type>
      <id>1</id>
      <width>200</width>
      <posx>660</posx>
      <posy>528</posy>
      <label>$LOCALIZE[435] $INFO[musicplayer.Playlistposition] / $INFO[musicplayer.Playlistlength]</label>
      <visible effect="fade" time="1000">Player.ShowInfo</visible>
      <align>right</align>
      <font>font10</font>
    </control>

  </controls>

</window>

