<window>
  <id>2006</id>
  <defaultcontrol>2</defaultcontrol>
  <allowoverlay>no</allowoverlay>
  <controls>
    <control>
      <description>visualisation</description>
      <type>visualisation</type>
      <id>2</id>
      <posX>0</posX>
      <posY>0</posY>
      <width>720</width>
      <height>576</height>
    </control>

    <!-- codec & viz infos -->

    <control>
      <description>preset background image</description>
      <type>image</type>
      <id>3</id>
      <posX>-10</posX>
      <posY>40</posY>
      <width>740</width>
      <height>65</height>
      <texture>osd_menu_bg.png</texture>
    </control>

    <control>
      <description>Codecinfo</description>
      <type>label</type>
      <id>3</id>
      <width>600</width>
      <posX>75</posX>
      <posY>50</posY>
      <label>Audio | Codec: $INFO(musicplayer.Codec) - Bitrate: $INFO(musicplayer.Bitrate)kbps - Bitdepth: $INFO(musicplayer.bitspersample)bit - Frequency: $INFO(musicplayer.Samplerate)kHz - Channels: $INFO(musicplayer.Channels)ch</label>
      <align>left</align>
      <font>font12</font>
    </control>

    <control>
      <description>Presets label</description>
      <type>label</type>
      <id>3</id>
      <posX>75</posX>
      <posY>75</posY>
      <width>560</width>
      <label>Visuals | </label>
      <font>font12</font>
      <align>left</align>
    </control>

    <control>
      <description>Presets label</description>
      <type>label</type>
      <id>3</id>
      <posX>150</posX>
      <posY>75</posY>
      <width>460</width>
      <label>$INFO(Visualisation.Name) - $INFO(Visualisation.Preset)</label>
      <font>font12</font>
      <align>left</align>
    </control>

    <control>
      <type>image</type>
      <description>Preset locked image</description>
      <id>4</id>
      <posX>134</posX>
      <posY>77</posY>
      <width>12</width>
      <height>15</height>
      <visible>Visualisation.Locked</visible>
      <texture>arenaitem-private.png</texture>
    </control>

    <!-- media infos -->

    <control>
      <description>media info background image</description>
      <type>image</type>
      <id>1</id>
      <posX>-10</posX>
      <posY>455</posY>
      <width>740</width>
      <height>100</height>
      <texture>osd_menu_bg.png</texture>
    </control>

    <control>
      <description>cover image</description>
      <type>image</type>
      <id>1</id>
      <posX>75</posX>
      <posY>455</posY>
      <width>100</width>
      <height>100</height>
      <info>MusicPlayer.Cover</info>
      <keepaspectratio>true</keepaspectratio>
    </control>

    <control>
      <description>Artist label</description>
      <type>label</type>
      <id>1</id>
      <width>445</width>
      <posX>157</posX>
      <posY>458</posY>
      <info>MusicPlayer.Artist</info>
      <align>left</align>
      <font>special12</font>
    </control>

    <control>
      <description>Title label</description>
      <type>label</type>
      <id>1</id>
      <width>430</width>
      <posX>157</posX>
      <posY>479</posY>
      <info>MusicPlayer.Title</info>
      <align>left</align>
      <font>special13</font>
    </control>

    <control>
      <description>Album label</description>
      <type>label</type>
      <id>1</id>
      <width>430</width>
      <posX>157</posX>
      <posY>507</posY>
      <info>MusicPlayer.Album</info>
      <align>left</align>
      <font>font10</font>
    </control>

    <control>
      <description>time label</description>
      <type>label</type>
      <id>1</id>
      <width>430</width>
      <posX>157</posX>
      <posY>528</posY>
      <label>$INFO(musicplayer.Time) / $INFO(musicplayer.Duration)</label>
      <align>left</align>
      <font>font10</font>
    </control>

    <control>
      <description>Playlist</description>
      <type>label</type>
      <id>1</id>
      <width>100</width>
      <posX>660</posX>
      <posY>528</posY>
      <label>Track $INFO(musicplayer.Playlistposition) / $INFO(musicplayer.Playlistlength)</label>
      <align>right</align>
      <font>font10</font>
    </control>

  </controls>

</window>

