<window id="2006">
	<defaultcontrol>-</defaultcontrol>
	<allowoverlay>no</allowoverlay>
	<controls>
		<control type="visualisation" id="2">
			<description>visualisation</description>
			<posx>0</posx>
			<posy>0</posy>
			<width>720</width>
			<height>576</height>
		</control>
		<!-- codec & viz infos -->
		<control type="group" id="0">
			<visible>Player.ShowCodec + !Window.IsVisible(script-XBMC_Lyrics-main.xml)</visible>
			<animation effect="fade" time="500">VisibleChange</animation>
			<control type="image" id="0">
				<description>preset background image</description>
				<posx>-10</posx>
				<posy>40</posy>
				<width>740</width>
				<height>90</height>
				<texture>osd_menu_bg.png</texture>
			</control>
			<control type="label" id="0">
				<description>Codecinfo</description>
				<width>630</width>
				<posx>50</posx>
				<posy>50</posy>
				<label>Audio | $INFO[musicplayer.Codec,Codec: ,]$INFO[musicplayer.Bitrate, - Bitrate: ,kbps]$INFO[musicplayer.bitspersample, - Bits:,bit]$INFO[musicplayer.Samplerate, - Freq.: ,kHz]$INFO[musicplayer.Channels, - Channels: ]</label>
				<align>left</align>
				<font>font12</font>
			</control>
			<control type="label" id="0">
				<description>Presets label</description>
				<posx>50</posx>
				<posy>75</posy>
				<width>630</width>
				<label>Visual | fps: $INFO[System.FPS] - $LOCALIZE[250] - $INFO[Visualisation.Name] - $LOCALIZE[20166]</label>
				<font>font12</font>
				<align>left</align>
				<visible>Visualisation.Locked</visible>
			</control>
			<control type="label" id="0">
				<description>Presets label</description>
				<posx>50</posx>
				<posy>75</posy>
				<width>630</width>
				<label>Visual | fps: $INFO[System.FPS] - $LOCALIZE[250] - $INFO[Visualisation.Name]</label>
				<font>font12</font>
				<align>left</align>
				<visible>!Visualisation.Locked</visible>
			</control>
			<control type="label" id="0">
				<description>Presets label</description>
				<posx>50</posx>
				<posy>100</posy>
				<width>630</width>
				<label>$LOCALIZE[13388] | $INFO[Visualisation.Preset]</label>
				<font>font12</font>
				<align>left</align>
			</control>
		</control>
		<!-- media infos -->
		<control type="group" id="1">
			<posx>-10</posx>
			<posy>410</posy>
			<visible>Player.ShowInfo + !Window.IsVisible(script-XBMC_Lyrics-main.xml)</visible>
			<animation effect="fade" time="500">VisibleChange</animation>
			<control type="image" id="1">
				<description>media info background image</description>
				<posx>0</posx>
				<posy>0</posy>
				<width>740</width>
				<height>120</height>
				<texture>osd_menu_bg.png</texture>
			</control>
			<control type="image" id="1">
				<description>cover image</description>
				<posx>50</posx>
				<posy>0</posy>
				<width>100</width>
				<height>120</height>
				<info>MusicPlayer.Cover</info>
				<aspectratio>keep</aspectratio>
			</control>
			<control type="label" id="1">
				<description>Current System Time label</description>
				<width>100</width>
				<posx>680</posx>
				<posy>5</posy>
				<info>System.Time</info>
				<align>right</align>
				<textcolor>green</textcolor>
				<font>special13</font>
			</control>
			<control type="fadelabel" id="1">
				<description>Artist label</description>
				<width>420</width>
				<posx>160</posx>
				<posy>5</posy>
				<info>MusicPlayer.Artist</info>
				<align>left</align>
				<font>special12</font>
			</control>
			<control type="fadelabel" id="1">
				<description>Title label</description>
				<width>420</width>
				<posx>160</posx>
				<posy>30</posy>
				<info>MusicPlayer.Title</info>
				<align>left</align>
				<font>special13</font>
			</control>
			<control type="fadelabel" id="1">
				<description>Album label</description>
				<width>420</width>
				<posx>160</posx>
				<posy>60</posy>
				<label>$INFO[MusicPlayer.Album]$INFO[musicplayer.discnumber, - $LOCALIZE[427]]</label>
				<align>left</align>
				<font>font10</font>
			</control>
			<control type="image" id="1">
				<description>Song Rating Image</description>
				<height>20</height>
				<width>100</width>
				<posx>585</posx>
				<posy>55</posy>
				<info>musicplayer.rating</info>
			</control>
			<control type="label" id="1">
				<description>time label</description>
				<width>520</width>
				<posx>160</posx>
				<posy>80</posy>
				<label>$INFO[musicplayer.Time]$INFO[musicplayer.Duration, / ]</label>
				<align>left</align>
				<font>font10</font>
			</control>
			<control type="label" id="1">
				<description>Playlist Pos No</description>
				<width>200</width>
				<posx>680</posx>
				<posy>80</posy>
				<label>$INFO[musicplayer.Playlistposition,$LOCALIZE[435] ]$INFO[musicplayer.Playlistlength, / ]</label>
				<visible>!MusicPartyMode.Enabled</visible>
				<align>right</align>
				<font>font10</font>
			</control>
			<control type="label" id="1">
				<description>PartyMode Enabled</description>
				<width>200</width>
				<posx>680</posx>
				<posy>80</posy>
				<label>589</label>
				<visible>MusicPartyMode.Enabled</visible>
				<align>right</align>
				<font>font10</font>
			</control>
			<control type="progress" id="1">
				<description>Progressbar</description>
				<posx>160</posx>
				<posy>105</posy>
				<width>520</width>
				<height>5</height>
				<info>Player.Progress</info>
				<texturebg>progress_back.png</texturebg>
				<lefttexture>progress_left.png</lefttexture>
				<midtexture>progress_mid.png</midtexture>
				<righttexture>progress_right.png</righttexture>
				<overlaytexture>-</overlaytexture>
				<visible>true</visible>
			</control>
		</control>
	</controls>
</window>