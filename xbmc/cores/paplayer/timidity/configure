	test -n "$SHLD" || SHLD="ld -Bshareable"
    else
	SHCFLAGS=-aou
	SHLDFLAGS="-L/usr/X11R6/lib/aout $SHLDFLAGS"
	test -n "$SHLD" || SHLD="env OBJFORMAT=aout ld -shared"
    fi
        so="so"
    ;;

