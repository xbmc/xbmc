        ;;
      "xwcc386_w.sh "*)
         WATCOM_C=yes
         CFLAGS="$CFLAGS -br -5s -zk0 -bm -zp=16 -w=4 -D_WINDOWS -DWIN32 -DINCLUDE_WINDOWS_H"
