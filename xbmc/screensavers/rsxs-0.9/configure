    "src/flux/Makefile") CONFIG_FILES="$CONFIG_FILES src/flux/Makefile" ;;
    "src/helios/Makefile") CONFIG_FILES="$CONFIG_FILES src/helios/Makefile" ;;
    "src/hyperspace/Makefile") CONFIG_FILES="$CONFIG_FILES src/hyperspace/Makefile" ;;
    "src/lattice/Makefile") CONFIG_FILES="$CONFIG_FILES src/lattice/Makefile" ;;
    "src/plasma/Makefile") CONFIG_FILES="$CONFIG_FILES src/plasma/Makefile" ;;
    "src/skyrocket/Makefile") CONFIG_FILES="$CONFIG_FILES src/skyrocket/Makefile" ;;
    "src/solarwinds/Makefile") CONFIG_FILES="$CONFIG_FILES src/solarwinds/Makefile" ;;
    "rsxs.spec") CONFIG_FILES="$CONFIG_FILES rsxs.spec" ;;

  *) as_fn_error "invalid argument: \`$ac_config_target'" "$LINENO" 5;;
  esac
