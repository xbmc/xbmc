-static void generatePrecalCoef (int precalCoef[16][16])
-{
-    int coefh, coefv;
-    
-    for (coefh = 0; coefh < 16; coefh++) {
-        for (coefv = 0; coefv < 16; coefv++) {
-            
-            int i;
-            int diffcoeffh;
-            int diffcoeffv;
-            
-            diffcoeffh = sqrtperte - coefh;
-            diffcoeffv = sqrtperte - coefv;
-            
-            if (!(coefh || coefv)) {
-                i = 255;
-            }
-            else {
-                int i1, i2, i3, i4;
-                
-                i1 = diffcoeffh * diffcoeffv;
-                i2 = coefh * diffcoeffv;
-                i3 = diffcoeffh * coefv;
