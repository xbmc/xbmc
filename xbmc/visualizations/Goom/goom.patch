-			vage = 0.001f;
-			radius += 1.0f;
-			mx = info->screen.width / 2;
-			gravity = 0.04f;
-			break;
-		default:
-			return;
-			/* my = i R A N D (info->screen.height); vage = 0.01f; */
-	}
