-        char *ptr = msg;
-        int pos;
-        float ecart;
-        message = msg;
-        sprintf (msg, goomInfo->update_message.message);
-        
-        while (!fin) {
-            while (1) {
-                if (*ptr == 0) {
-                    fin = 1;
-                    break;
-                }
-                if (*ptr == '\n') {
-                    *ptr = 0;
-                    break;
-                }
-                ++ptr;
-            }
-            pos = goomInfo->update_message.affiche - (goomInfo->update_message.numberOfLinesInMessage - i)*25;
