<scraper name="TV.com" content="tvshows" thumb="tvcom.jpg">
	<CreateSearchUrl dest="3">
		<RegExp input="$$1" output="&lt;url spoof=&quot;http://www.tv.com&quot;&gt;http://www.tv.com/search.php?stype=program&amp;amp;qs=\1&amp;amp;tag=tv_shows&lt;/url&gt;" dest="3">
			<expression></expression>	
		</RegExp>
	</CreateSearchUrl>
	<GetSearchResults dest="3">
		<RegExp input="$$4" output="&lt;results&gt;\1&lt;/results&gt;" dest="3">
			<RegExp input="$$1" output="&lt;entity&gt;&lt;title&gt;\2&lt;/title&gt;&lt;url spoof=&quot;http://www.tv.com&quot;&gt;http://www.tv.com/show/\1/summary.html&lt;/url&gt;&lt;url spoof=&quot;http://www.tv.com&quot;&gt;http://www.tv.com/show/\1/cast.html&lt;/url&gt;&lt;id&gt;\1&lt;/id&gt;&lt;/entity&gt;" dest="4">
				<expression repeat="yes">Show: &lt;a class=&quot;f-bold f-C30&quot; href=&quot;http://www\.tv\.com/[^/]*/show/([0-9]+)/[^&quot;]*&quot;&gt;([^&lt;]*)&lt;/a&gt;</expression>
			</RegExp>	
			<expression noclean="1"></expression>
		</RegExp>	
	</GetSearchResults>	
<!-- 	returns: results in xml format <details><plot>*</plot><director>*</director><year>*</year><episodeguide>*</episodeguide></details> -->	
	<GetDetails dest="7">
		<RegExp input="$$5" output="&lt;details&gt;\1&lt;/details&gt;" dest="7">
			<RegExp input="$$1" output="&lt;title&gt;\1&lt;/title&gt;" dest="5">
				<expression>&lt;h1&gt;([^&lt;]*)&lt;/h1&gt;[^&lt;]*&lt;div id=&quot;content-tabs&quot;&gt;</expression>
			</RegExp>		
			<RegExp input="$$1" output="&lt;plot&gt;\1&lt;/plot&gt;" dest="5+">
				<expression>'Close Full Summary \[-\]', '([^']*)</expression>
			</RegExp>
			<RegExp input="$$1" output="&lt;runtime&gt;\1 min&lt;/runtime&gt;" dest="5+">
				<expression>\(([0-9]*) min.\)</expression>
			</RegExp>		
			<RegExp input="$$1" output="\1" dest="6">
				<expression>Show Categories:(.*)&lt;div class=&quot;mt-10&quot;&gt;</expression>
			</RegExp>			
			<RegExp input="$$6" output="&lt;genre&gt;\1&lt;/genre&gt;" dest="5+">
				<expression repeat="yes">&quot;&gt;([^&lt;]*)&lt;/a&gt;</expression>
			</RegExp>
			<RegExp input="$$1" output="&lt;rating&gt;\1&lt;/rating&gt;" dest="5+">
				<expression>Score:&lt;/span&gt;[^&lt;]*&lt;span[^&gt;]*&gt;([0-9\.]*)&lt;/span&gt;</expression>
			</RegExp>			
			<RegExp input="$$1" output="&lt;votes&gt;\1&lt;/votes&gt;" dest="5+">
				<expression>&lt;span class=&quot;f-11&quot;&gt;([0-9,]*) votes&lt;/span&gt;</expression>
			</RegExp>											
			<RegExp input="$$2" output="&lt;actor&gt;&lt;name&gt;\1&lt;/name&gt;&lt;role&gt;\2&lt;/role&gt;&lt;/actor&gt;"  dest="5+">
				<expression repeat="yes">\?tag=stars[^&gt;]*&gt;([^&lt;]*)&lt;/a&gt;&lt;br /&gt;[^&lt;]*&lt;span class=&quot;f-bold&quot;&gt;Role: ([^&lt;]*)&lt;/span&gt;</expression>
			</RegExp>						
			<RegExp input="$$1" output="&lt;episodeguide spoof=&quot;http://www.tv.com&quot;&gt;http://www.tv.com/show/$$3/episode_listings.html&lt;/episodeguide&gt;" dest="5+">
				<expression></expression>
			</RegExp>	
			<expression noclean="1"></expression>
		</RegExp>		
	</GetDetails>	
	<!-- 	returns: results in xml format <episodeguide><episode><title>*</title><url>*</url><season>*</season><epnum>*</epnum><thumb>*</thumb><id>*</id></episode>*</episodeguide> !-->	
	<GetEpisodeList dest="3">
		<RegExp input="$$5" output="&lt;episodeguide&gt;\1&lt;/episodeguide&gt;" dest="3">
			<RegExp input="$$1" output="&lt;episode&gt;&lt;title&gt;\2&lt;/title&gt;&lt;id&gt;\1&lt;/id&gt;&lt;url&gt;http://www.tv.com/episode/\1/summary.html&lt;/url&gt;&lt;/episode&gt;" dest="5">
				<expression repeat="yes">/episode/([0-9]*)/summary\.html\?tag=ep_list[^&gt;]*&gt;([^&lt;]*)&lt;/a&gt;</expression>
			</RegExp>				
			<expression noclean="1"></expression>
		</RegExp>			
	</GetEpisodeList>
<!-- 	returns: results in xml format <details><writer>*</writer><director>*</director><cast>*</cast><rating>*</rating><rank>*</rank><plot>*</plot> -->	
	<GetEpisodeDetails dest="3">
		<RegExp input="$$5" output="&lt;details&gt;\1&lt;/details&gt;" dest="3">
			<RegExp input="$$1" output="&lt;plot&gt;\1&lt;/plot&gt;" dest="5">
				<expression>&lt;div id=&quot;main-col&quot;&gt;[^&lt;]*&lt;div&gt;(.*)&lt;div class=&quot;ta-r mt-10 f-bold&quot;&gt;</expression>
			</RegExp>	
<!--			<RegExp input="$$1" output="&lt;plot&gt;\1&lt;/plot&gt;" dest="5+">
				<expression>a</expression>
			</RegExp>	
			<RegExp input="$$1" output="&lt;plot&gt;\1&lt;/plot&gt;" dest="5+">
				<expression>a</expression>
			</RegExp>						-->			
			<expression noclean="1"></expression>
		</RegExp>		
	</GetEpisodeDetails>	
</scraper>