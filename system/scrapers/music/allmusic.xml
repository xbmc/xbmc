			<RegExp input="$$1" output="\1" dest="7">
				<expression noclean="1">Style Listing--&gt;([^!]*)&lt;!--Style Listing</expression>
			</RegExp>
			<RegExp input="$$7" output="&lt;style&gt;\1&lt;/style&gt;" dest="5+">
