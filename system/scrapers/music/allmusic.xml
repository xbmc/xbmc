					<expression noclean="1" repeat="yes">&lt;a href[^&gt;]*&gt;([^&lt;]*)&lt;/a&gt;</expression>
				</RegExp>
				<expression noclean="1" repeat="yes"></expression>
			</RegExp>
			<RegExp input="$$1" output="\1" dest="7">
				<expression noclean="1">&lt;\!--Styles Listing--&gt;([^\!]*)&lt;\!--Styles Listing--&gt;</expression>
			</RegExp>
			<RegExp input="$$2" output="\1" dest="5+">
				<RegExp input="$$7" output="&lt;style&gt;\1&lt;/style&gt;" dest="2+">
					<expression noclean="1" repeat="yes">&lt;a href[^&gt;]*&gt;([^&lt;]*)&lt;/a&gt;</expression>
				</RegExp>
				<expression noclean="1" repeat="yes"></expression>
			</RegExp>
			<RegExp input="$$1" output="\1" dest="8">
				<expression noclean="1">&lt;\!--Begin Moods Listing--&gt;([^\!]*)&lt;\!--Moods Listing--&gt;</expression>
