				<expression repeat="yes" clear="yes">&lt;td class=&quot;sorted-cell&quot;&gt;([0-9]*)&lt;/td&gt;&lt;td class=&quot;cell-img&quot;&gt;[^&gt;]*&gt;[^\?]*[^&gt;]*&gt;([^&lt;]*)&lt;/a&gt;&lt;/td&gt;&lt;td class=&quot;cell-img&quot;&gt;[^&gt;]*&gt;[^\?]*break-word;&quot;&gt;([^&lt;]*)&lt;/td&gt;&lt;/tr&gt;</expression>
			</RegExp>
			<expression noclean="1"></expression>
		</RegExp>
