			</RegExp>
			<RegExp input="$$1" output="\1" dest="6">
				<expression noclean="1">&lt;\!--Begin Genre Listing--&gt;([^\!]*)&lt;\!--Genre Listing--&gt;</expression>
			</RegExp>
			<RegExp input="$$2" output="\1" dest="5+">
