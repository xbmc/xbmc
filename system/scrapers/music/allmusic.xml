				</RegExp>
				<expression noclean="1" repeat="yes"></expression>
			</RegExp>
			<RegExp input="$$1" output="\1" dest="9">
				<expression noclean="1">&lt;\!--Themes Listing--&gt;([^\!]*)&lt;\!--Themes Listing--&gt;</expression>
			</RegExp>
			<RegExp input="$$2" output="\1" dest="5+">
