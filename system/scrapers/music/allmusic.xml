				</RegExp>
				<expression noclean="1" repeat="yes"></expression>
			</RegExp>
			<RegExp input="$$1" output="&lt;id&gt;\1&lt;/id&gt;" dest="5+">
				<expression noclean="1">&lt;span&gt;AMG Album ID&lt;/span&gt;[^;]*[^&gt;]*&gt;&lt;pre&gt;([^&lt;]*)&lt;/pre&gt;</expression>
			</RegExp>
			<RegExp input="$$1" output="&lt;track&gt;&lt;position&gt;\1&lt;/position&gt;&lt;title&gt;\2&lt;/title&gt;&lt;duration&gt;\3&lt;/duration&gt;&lt;/track&gt;" dest="5+">
				<expression noclean="1,2,3" repeat="yes">&lt;TD class=&quot;cell&quot;&gt;([0-9]*)&lt;/TD&gt;[^&lt;]*&lt;TD[^&gt;]*&gt;[^&lt;]*&lt;a[^&gt;]*&gt;&lt;img[^&gt;]*&gt;&lt;/a&gt;&lt;/td&gt;[^&lt;]*[^&gt;]*&gt;[^&gt;]*&gt;([^&lt;]*)[^&quot;]*&lt;[^&lt;]*[^&quot;]*[^&gt;]*&gt;([^&lt;]*)&lt;</expression>
			</RegExp>
			<RegExp input="$$1" output="&lt;thumb&gt;http://image.allmusic.com/\1&lt;/thumb&gt;" dest="5+">
				<expression noclean="1">http://image.allmusic.com/([^&quot;]*)&quot;</expression>
			</RegExp>
			<RegExp input="$$1" output="&lt;url function=&quot;GetReview&quot;&gt;http://www.allmusic.com/cg/amg.dll?p=amg&amp;amp;sql=\1&lt;/url&gt;" dest="5+">
				<expression noclean="1">&lt;a HREF=&quot;[^:]*sql=([^&quot;]*)&quot;&gt;Review&lt;/a&gt;&lt;/td&gt;</expression>
			</RegExp>
			<expression noclean="1"></expression>
		</RegExp>
