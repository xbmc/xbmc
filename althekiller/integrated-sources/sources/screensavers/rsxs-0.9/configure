echo "  XScreenSaver support      :  $HAVE_PKG_XSCREENSAVER" >&2
echo "  PNG image support         :  $HAVE_IMAGE" >&2
echo "  OGG Vorbis sound support  :  $HAVE_SOUND" >&2
