ac_subst_vars='SHELL PATH_SEPARATOR PACKAGE_NAME PACKAGE_TARNAME PACKAGE_VERSION PACKAGE_STRING PACKAGE_BUGREPORT exec_prefix prefix program_transform_name bindir sbindir libexecdir datadir sysconfdir sharedstatedir localstatedir libdir includedir oldincludedir infodir mandir build_alias host_alias target_alias DEFS ECHO_C ECHO_N ECHO_T LIBS INSTALL_PROGRAM INSTALL_SCRIPT INSTALL_DATA CYGPATH_W PACKAGE VERSION ACLOCAL AUTOCONF AUTOMAKE AUTOHEADER MAKEINFO install_sh STRIP ac_ct_STRIP INSTALL_STRIP_PROGRAM mkdir_p AWK SET_MAKE am__leading_dot AMTAR am__tar am__untar RELEASE CXX CXXFLAGS LDFLAGS CPPFLAGS ac_ct_CXX EXEEXT OBJEXT DEPDIR am__include am__quote AMDEP_TRUE AMDEP_FALSE AMDEPBACKSLASH CXXDEPMODE am__fastdepCXX_TRUE am__fastdepCXX_FALSE CXXCPP CC CFLAGS ac_ct_CC CCDEPMODE am__fastdepCC_TRUE am__fastdepCC_FALSE RANLIB ac_ct_RANLIB EGREP LIBADD_DL build build_cpu build_vendor build_os host host_cpu host_vendor host_os LN_S GL_COND_LIBTOOL_TRUE GL_COND_LIBTOOL_FALSE CPP ALLOCA ALLOCA_H LIBOBJS GETOPT_H ARGZ_H STDBOOL_H HAVE__BOOL SYSEXITS_H UNISTD_H EOVERFLOW X_CFLAGS X_PRE_LIBS X_LIBS X_EXTRA_LIBS X_CPPFLAGS HACKMODE_TRUE HACKMODE_FALSE XSCREENSAVER4_TRUE XSCREENSAVER4_FALSE SYMLINKS_TRUE SYMLINKS_FALSE xscreensaver defaultdir hackdir configdir OPENGL_LIBS HAVE_SOUND SOUND_CPPFLAGS SOUND_CXXFLAGS SOUND_LDFLAGS SOUND_LIBS SOUND_TRUE SOUND_FALSE DLOPEN_TRUE DLOPEN_FALSE HAVE_IMAGE IMAGE_CPPFLAGS IMAGE_CXXFLAGS IMAGE_LDFLAGS IMAGE_LIBS IMAGE_TRUE IMAGE_FALSE hackreldir BUILD_CYCLONE_TRUE BUILD_CYCLONE_FALSE BUILD_EUPHORIA_TRUE BUILD_EUPHORIA_FALSE BUILD_FIELDLINES_TRUE BUILD_FIELDLINES_FALSE BUILD_FLOCKS_TRUE BUILD_FLOCKS_FALSE BUILD_FLUX_TRUE BUILD_FLUX_FALSE BUILD_HELIOS_TRUE BUILD_HELIOS_FALSE BUILD_HYPERSPACE_TRUE BUILD_HYPERSPACE_FALSE BUILD_LATTICE_TRUE BUILD_LATTICE_FALSE BUILD_PLASMA_TRUE BUILD_PLASMA_FALSE BUILD_SKYROCKET_TRUE BUILD_SKYROCKET_FALSE BUILD_SOLARWINDS_TRUE BUILD_SOLARWINDS_FALSE ENABLED_HACKS ENABLED_BINARIES ENABLED_CONFIGS DISABLED_HACKS LTLIBOBJS'
